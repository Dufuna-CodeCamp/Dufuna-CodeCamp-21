{
    "require": {
        "slim/slim": "4.*",
        "slim/psr7": "^1.5",
        "illuminate/database": "*",
        "vlucas/phpdotenv": "^5.4"
    },

    "autoload": {
        "psr-4": {
            "App\\Controller\\": "app/controllers",
            "App\\Middleware\\": "app/middlewares",
            "App\\Model\\": "app/models"
        }
    },


    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
